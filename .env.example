# AWS Bedrock Configuration
VITE_AWS_REGION=us-east-1
VITE_AWS_ACCESS_KEY_ID=your_access_key_here
VITE_AWS_SECRET_ACCESS_KEY=your_secret_key_here

# AWS Bedrock Model Configuration
# Use cross-region inference profiles (required for on-demand throughput)
# Claude 3.5 Sonnet v2 (latest, recommended)
VITE_BEDROCK_MODEL_ID=us.anthropic.claude-3-5-sonnet-v2:0

# Other available inference profiles:
# VITE_BEDROCK_MODEL_ID=us.anthropic.claude-3-5-sonnet-v1:0  # Claude 3.5 Sonnet v1
# VITE_BEDROCK_MODEL_ID=us.anthropic.claude-3-opus-v1:0      # Claude 3 Opus
# VITE_BEDROCK_MODEL_ID=us.anthropic.claude-3-sonnet-v1:0    # Claude 3 Sonnet
# VITE_BEDROCK_MODEL_ID=us.anthropic.claude-3-haiku-v1:0     # Claude 3 Haiku

# Optional: Use mock data for development (set to 'true' to skip AWS calls)
VITE_USE_MOCK_AI=false
